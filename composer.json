{
    "name": "veseluy-rodjer/supercrud",
    "license": "MIT",
    "description": "Package for using crud in adminka in Laravel 5.7",
    "keywords": [
        "laravel",
        "crud",
        "crud generator",
        "laravel crud generator",
        "api generator"
    ],
    "authors": [
        {
            "name": "Nikolay M",
            "email": "nikolas-ja@meta.ua"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.1",
        "laravelcollective/html": "^5.1",
        "veseluy-rodjer/supercrud": "master"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.6",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "VeseluyRodjer\\CrudGenerator\\": "src/"
        },
        "classmap": [
            "tests"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "VeseluyRodjer\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    }
}
